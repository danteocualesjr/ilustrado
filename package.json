  "description": "Monorepo with client (React) and server (Express)",
  "workspaces": [
    "client",
    "server"
  ],
  "dependencies": {
    "@prisma/client": "^6.11.1",
    "bcryptjs": "^3.0.2",
    "cors": "^2.8.5",
    "dotenv": "^17.2.0",
    "express": "^5.1.0",
